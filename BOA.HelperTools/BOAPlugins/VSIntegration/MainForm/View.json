{
  "Title": "Close -> Esc",
  "Width": 450,
  "Height": 540,
  "Content": {

    "ui": "StackPanel",
    "Childs": [
      {
        "ui": "Button",
        "Text": "Check In Current Solution",
        "Click": "this.FireAction('CheckInSolution')",
        "IsVisible": "{Binding CheckInSolutionIsEnabled}",
        "Height": 20,
        "Margin": 10
      },

      {
        "ui": "Button",
        "Text": "Update 'models -> AutoGeneratedModels.tsx'",
        "Click": "this.FireAction('UpdateTypeScriptModels')",
        "Margin": 10
      },

      {
        "ui": "Button",
        "Text": "Update Message.cs (in type project)",
        "Click": "this.FireAction('UpdateMessageCs')",
        "Margin": 10
      },
      {
        "ui": "Button",
        "Text": "Remove Unused Messages In Message.cs (in type project)",
        "Click": "this.FireAction('RemoveUnusedMessagesInCsCodes')",
        "Margin": 10
      },
      {
        "ui": "Button",
        "Text": "Update 'utils -> Message.tsx'",
        "Click": "this.FireAction('UpdateMessageTsx')",
        "Margin": 10
      },
      {
        "ui": "Button",
        "Text": "Remove Unused Messages In Typescript Codes ( 'utils -> Message.tsx' )",
        "Click": "this.FireAction('RemoveUnusedMessagesInTypescriptCodes')",
        "Margin": 10
      },
      {
        "ui": "Button",
        "Text": "{Binding MethodCallGraphButtonText}",
        "Click": "this.FireAction('ViewMethodCallGraph')",
        "IsVisible": "{Binding MethodCallGraphIsVisible}",
        "Margin": 10
      },
      {
        "ui": "Button",
        "Text": "View type dependency of selected project",
        "Click": "this.FireAction('ViewTypeDependencyOfSelectedProject')",
        "IsVisible": "{Binding ViewTypeDependencyOfSelectedProjectIsVisible}",
        "Margin": 10
      },
      {
        "ui": "Button",
        "Text": "Open Plugin Directory",
        "Click": "this.FireAction('OpenPluginDirectory')",
        "Margin": 10
      },
      {
        "ui": "Button",
        "Text": "Update 'Extensions.designer.cs' file in orchestration project",
        "Click": "this.FireAction('InitializeFormAssistantDefaultCodes')",
        "Margin": 10
      },
      {
        "ui": "TextBox",
        "Text": "{Binding SolutionCheckInComment}",
        "Label": "Check In Comment",
        "IsVisible": "{Binding CheckInSolutionIsEnabled}",
        "Margin": 10
      }
    ]


  }
}