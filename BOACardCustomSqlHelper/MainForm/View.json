{
  "Title": "BOA Card Custom Sql Helper",
  "Width": 900,
  "Height": 800,
  "Content": {
    "ui": "Grid",
    "rows": [

      {
        "ui": "StackPanel",
        "Name": "ActionPanel",
        "gravity": 1,
        "FlowDirection": "RightToLeft",
        "Orientation": "Horizontal"
      },
      { "ui": "GridSplitter" },
      {
        "gravity": 15,
        "ui": "Grid",
        "rows": [
          {
            "ui": "GroupBox",
            "Header": "Search",
            "IsVisible": "{Binding SearchIsVisible}",
            "Content": {

              "ui": "StackPanel",
              "Width": 500,
              "vAlignIsCenter": true,
              "Margin": 20,
              "rows": [
                {
                  "ui": "TextBox",
                  "Label": "Profile Id",
                  "IntellisenseSource": "{Binding ProfileIdCollection}",
                  "Text": "{Binding ProfileId}",
                  "vAlignIsCenter": true,
                  "TextChanged": "this.FireAction('OnProfileIdChanged')",
                  "MarginTop": 10
                }
              ]
            }

          }
        ]
      }


    ]
  }
}
