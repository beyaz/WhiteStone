{
  "Title": "Entity Generator",
  "Width": 300,
  "Height": 270,
  "Content": {
    "ui": "Grid",
    "rows": [
      {
        "view": "StackPanel",
        "Margin": 10,
        "Rows": [
          {
            "view": "Textbox",
            "Text": "{Binding CheckInComment}",
            "Label": "Tfs check-in Comment"
          },
          {
            "view": "Textbox",
            "Text": "{Binding SchemaName}",
            "Label": "Schema"
          },

          {
            "view": "ProgressBar",
            "Minimum": 0,
            "Maximum": 100,
            "Value": "{Binding ProcessIndicatorValue}" ,
            "MarginTop": 10
          },
          {
            "view": "Label",
            "Content": "{Binding ProcessIndicatorText}"
          },
          {
            "ui": "StackPanel",
            "Name": "ActionPanel",
            "FlowDirection": "RightToLeft",
            "Orientation": "Horizontal",
            "MarginTop": 10
          }
        ]
      }

    ]
  }
}
